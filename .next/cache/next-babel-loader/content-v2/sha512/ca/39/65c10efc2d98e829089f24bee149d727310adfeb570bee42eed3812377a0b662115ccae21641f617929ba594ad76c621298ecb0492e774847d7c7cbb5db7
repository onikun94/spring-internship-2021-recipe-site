\n      if (id === 0 || isNaN(id)) {\n        console.log(\"nullでした\");\n        setRecipe(null);\n      } else {\n        const recipe = await getRecipe(id);\n        console.log(\"レシピが入ります\");\n        setRecipe(recipe);\n      }\n    })();\n  }, [router.query.id]);\n  return (\n    <div>\n      <h1>My Recipe Site{recipe?.id}</h1>\n\n      {recipe && (\n        <main>\n          <h2>{recipe.title}</h2>\n\n          {recipe.image_url && (\n            <img src={recipe.image_url} alt=\"レシピ画像\" width=\"300\" />\n          )}\n\n          <p>{recipe.description}</p>\n\n          <h3>材料</h3>\n          <ol>\n            {recipe.ingredients.map((ing, i) => (\n              <li key={i}>\n                {ing.name} : {ing.quantity}\n              </li>\n            ))}\n          </ol>\n\n          <h3>手順</h3>\n          <ol>\n            {recipe.steps.map((step, i) => (\n              <li key={i}>{step}</li>\n            ))}\n          </ol>\n        </main>\n      )}\n    </div>\n  );\n};\n\nexport default RecipePage;\n"]},"metadata":{},"sourceType":"module"}